{
  "name": "doctor-web",
  "productName": "doctor-web",
  "version": "1.2.0",
  "description": "医生端",
  "author": "吴晓然",
  "main": "main.js",
  "dependencies": {
    "pretty-bytes": "^2.0.1"
  },
  "devDependencies": {
    "cz-conventional-changelog": "^2.0.0",
    "electron": "^1.6.11",
    "electron-builder": "^19.16.1",
    "electron-packager": "^8.7.2"
  },
  "scripts": {
    "start": "electron ./src/main.js",
    "build-mac": "electron-builder --mac -c='electron-builder.json'",
    "pack-mac": "electron-packager ./src doctor-web --platform=darwin --icon=./icon.icns --out=../electron_build --overwrite",
    "build-win": "electron-builder --win32 -c='electron-builder.json'",
    "pack-win": "electron-packager ./ doctor-web --platform=win32 --arch=ia32 --overwrite"
  },
  "author": "",
  "license": "ISC",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "node": {
    "__dirname": false,
    "__filename": false
  }
}
